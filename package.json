  "license": "ISC",
  "scripts": {
    "start": "node config/ore.js",
    "clean": "NODE_ENV=clean node config/ore.js",
    "production": "NODE_ENV=production node config/ore.js"
  },
  "dependencies": {
    "browser-sync": "^2.14.0",
    "handlebars": "~4.0.5",
    "metalsmith": "~2.1.0",
    "metalsmith-assets": "~0.1.0",
    "metalsmith-in-place": "~1.3.2",
    "metalsmith-layouts": "~1.4.2",
    "metalsmith-permalinks": "~0.4.0",
    "metalsmith-sense-sass": "^1.1.0",
    "metalsmith-uglify": "^1.2.2",
    "metalsmith-watch": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiewilson/ore.git"
  }
