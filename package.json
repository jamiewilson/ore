{
  "name": "ore",
  "version": "1.0.0",
  "description": "A static site workflow that auto-reloads on file changes; synchronizes browsers; compiles, autoprefixes, minifies, and sourcemaps SCSS; concatenates and uglifies JS; provides an external URL for easy cross-device development.",
  "author": "Jamie Wilson",
  "license": "ISC",
  "scripts": {
    "start": "node config/ore.js",
    "clean": "NODE_ENV=clean node config/ore.js",
    "production": "NODE_ENV=production node config/ore.js"
  },
  "devDependencies": {
    "browser-sync": "^2.16.0",
    "handlebars": "~4.0.5",
    "metalsmith": "~2.2.0",
    "metalsmith-assets": "~0.1.0",
    "metalsmith-in-place": "~1.4.4",
    "metalsmith-layouts": "~1.6.5",
    "metalsmith-permalinks": "~0.5.0",
    "metalsmith-sense-sass": "^1.1.0",
    "metalsmith-static": "0.0.5",
    "metalsmith-uglify": "^1.5.1",
    "metalsmith-watch": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiewilson/ore.git"
  }
}
